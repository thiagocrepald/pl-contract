{
    "name": "plantao-mais",
    "version": "1.0.0",
    "description": "React version of Argon Dashboard by Creative Tim",
    "main": "index.js",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/creativetimofficial/argon-dashboard-react.git"
    },
    "keywords": [
        "react",
        "reactjs"
    ],
    "author": "Creative Tim",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/creativetimofficial/argon-dashboard-react/issues"
    },
    "homepage": "https://plantao-mais-hygea.appspot.com/",
    "scripts": {
        "start": "react-scripts start --watch",
        "start-dev": "env-cmd -f .env.development react-scripts start --watch",
        "start-staging": "env-cmd -f .env.staging react-scripts start --watch",
        "environment": "./env.sh",
        "build": "react-scripts build",
        "build:staging": "env-cmd -f .env.staging react-scripts build && yarn del-webapp && yarn deploy-react && yarn remove-build",
        "build:development": "env-cmd -f .env.development react-scripts build && yarn del-webapp && yarn deploy-react && yarn remove-build",
        "postbuild": "yarn del-webapp && yarn deploy-react && yarn remove-build",
        "del-webapp": "rm -R -f ./../webapp/ && mkdir ./../webapp",
        "deploy-react": "cp -a ./build/. ./../webapp/ && cp -a ./WEB-INF ./../webapp",
        "remove-build": "rm -R -f ./../webapp/build",
        "test": "react-scripts test",
        "lint": "tslint -c tslint.json 'src/**/*.{ts,tsx}'",
        "tslint-check": "tslint-config-prettier-check ./tslint.json",
        "eject": "react-scripts eject",
        "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm install && npm start",
        "compile-sass": "node-sass src/assets/scss/argon-dashboard-react.scss src/assets/css/argon-dashboard-react.css",
        "minify-sass": "node-sass src/assets/scss/argon-dashboard-react.scss src/assets/css/argon-dashboard-react.min.css --output-style compressed",
        "map-sass": "node-sass src/assets/scss/argon-dashboard-react.scss src/assets/css/argon-dashboard-react.css --source-map true"
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ],
    "dependencies": {
        "@date-io/date-fns": "^2.13.1",
        "@date-io/moment": "1.3.13",
        "@emotion/core": "10.0.10",
        "@hookform/resolvers": "^2.8.8",
        "@material-ui/core": "^4.11.3",
        "@material-ui/lab": "^4.0.0-alpha.57",
        "@material-ui/pickers": "^3.3.10",
        "@sentry/react": "^6.10.0",
        "@sentry/tracing": "^6.10.0",
        "@types/yup": "^0.29.13",
        "add": "^2.0.6",
        "axios": "0.18.0",
        "axios-observable": "1.0.7",
        "bootstrap": "^4.6.0",
        "c3": "^0.7.20",
        "chart.js": "2.7.3",
        "classnames": "2.2.6",
        "d3": "^6.5.0",
        "date-fns": "^2.28.0",
        "file-saver": "2.0.2",
        "http-status-codes": "^2.1.4",
        "i18next": "^19.8.8",
        "jwt-decode": "^3.1.2",
        "lodash": "^4.17.21",
        "mathjs": "5.10.3",
        "moment": "2.24.0",
        "node-sass": "4.14.1",
        "nouislider": "13.1.1",
        "postcss": "7.0.32",
        "rc-if": "0.0.11",
        "react": "16.8.4",
        "react-axiom": "0.4.0",
        "react-big-calendar": "^0.33.2",
        "react-bootstrap": "1.0.1",
        "react-bootstrap-sweetalert": "4.4.1",
        "react-chartjs-2": "2.7.4",
        "react-copy-to-clipboard": "5.0.1",
        "react-cropper": "1.3.0",
        "react-currency-input": "1.3.6",
        "react-currency-input-field": "^3.3.1",
        "react-datepicker": "2.5.0",
        "react-datetime": "2.16.3",
        "react-dom": "16.8.4",
        "react-file-base64": "1.0.3",
        "react-google-charts": "3.0.13",
        "react-google-maps": "9.4.5",
        "react-hook-form": "^7.27.1",
        "react-i18next": "^11.8.7",
        "react-icons": "2",
        "react-infinite-scroll-component": "^6.1.0",
        "react-input-mask": "2.0.4",
        "react-intl-currency-input": "^0.2.6",
        "react-loading-overlay": "1.0.1",
        "react-modal": "^3.12.1",
        "react-radio-group": "3.0.3",
        "react-router": "5.0.1",
        "react-router-dom": "^5.2.0",
        "react-select": "3.1.0",
        "react-show-more-text": "^1.4.6",
        "react-spinners": "0.5.4",
